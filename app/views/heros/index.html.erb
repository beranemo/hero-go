<h1>英雄列表</h1> 
<table>
  <tr>
    <td>編號</td>
    <td>名稱</td>
    <td>誕生日</td>
    <td>性別</td>
    <td>生命值</td>
    <td>職業</td>
    <td>VIP</td>
  </tr>
  <%  @heros.each do |h| %>
  <tr>
    <td><%= h.id %></td>
    <td><%= h.name %></td>
    <td><%= h.dob %></td>
    <td><%= h.gender %></td>
    <td><%= h.hp %></td>
    <td><%= h.hero_code %></td>
    <td><%= h.vip %></td>
  </tr>
  <% end %>
</table>